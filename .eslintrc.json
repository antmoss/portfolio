{
  "root": true,
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "arrow-parens": "off",
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  }
}
